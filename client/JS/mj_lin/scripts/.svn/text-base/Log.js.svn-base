var Log = function (obj, msg) {
    cc.log("[" + obj.tag + "]:" + msg);
}


var ERROR = function (obj, msg) {
    msg += "_____uid:" + ZJHModel.getInstance().uid + " name:" + ZJHModel.getInstance().name + " roomCode:" + MJModel.roomCode + " jushu:" + MJModel.currentCount;
    cc.log(msg);
    var ary = {};
    var dat = {};
    dat["httpCode"] = 100000;
    dat["codeDesc"] = "mj:" + MJModel.curGameType;
    dat["content"] = msg;
    dat["produceTime"] = parseInt(time(null) / 1000);
    dat["requestUrl"] = "";
    dat["requestParams"] = "";
    ary["logInfo"]="["+JSON.stringify(dat)+"]";
    CCHttpAgent.getInstance().sendHttpPost(null,"clientLog/upload",JSON.stringify(ary), "js_error");
}