            Utils.addCardSprites(__String.createWithFormat("minggang/cards/%1_%2_%3_debug", MJModel.mj_font, MJModel.mj_fontSize, MJModel.mj_table_view));
        } else if (MJModel.mj_font == "gexing" && MJModel.mj_fontSize == "dahao" && MJModel.mj_bg_use == 0) {
