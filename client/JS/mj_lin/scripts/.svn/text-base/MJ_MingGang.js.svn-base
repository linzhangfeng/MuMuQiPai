var sendXiaPao = function (v) {
    var packet = createPacket("proto.game.minggangmajiang.ResPaoFenResult");
    packet.paofen = v;
    sendPacket(packet, selfCMD.CLIENT_MJ_CHOOSE_PIAO);
};

