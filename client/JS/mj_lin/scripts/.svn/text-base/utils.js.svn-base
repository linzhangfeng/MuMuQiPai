    parseName: function (max, name) {
        try {
            var l = name.length;
            var blen = 0;
            var len = [];
            var n = 0;
            for (var i = 0; i < l; i++) {
                n = 0;
                if ((name.charCodeAt(i) & 0xff00) != 0) {
                    blen++;
                    n++;
                }
                blen++;
                n++;
                len[i] = n;
            }
            if (blen > max) {
                n = 0;
                for (var i = 0; i < l; i++) {
                    n += len[i];
                    if (n >= max) {
                        n = i + 1;
                        break;
                    }
                }
                return name.substr(0, n) + "..";
            }
            return name;
        } catch (e) {
            return "";
        }
    },
