var GetChild = function(string){
    try {
        var args = arguments;
        var parent = args[0];
        for (var i = 1; i < args.length; i ++){
            parent = parent.getChildByName(args[i]);
        }
        return parent;
    } catch (e) {
        cc.log("createWithFormat error : " + e);
        return null;
    }
};

