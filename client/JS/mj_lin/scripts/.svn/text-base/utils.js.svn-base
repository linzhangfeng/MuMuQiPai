    splitStr: function (str, max, maxL) {
        if (maxL == undefined)maxL = 2;
        if (str.length <= max){
            str.replace(","," ");
            return [str];
        }
        var str_array = str.split(" ");
        var result = [];
        var curStr = "";
        for (var i = 0; i < str_array.length; i++) {
            var str_i = str_array[i];
            if ((curStr.length + str_i.length) > max && result.length < (maxL - 1)) {
                result.push(curStr);
                curStr = "";
            }
            curStr += str_i + " ";
        }
        result.push(curStr);
        return result;
    },
