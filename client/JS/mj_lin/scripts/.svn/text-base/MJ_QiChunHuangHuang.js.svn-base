        MJCards.prototype.lockMyHandCards = function () {
            //开始锁牌
            for (var i = 0; i < this.hand_card[0].length; i++) {
                var card = this.hand_card[0][i];
                card.setCardTouchEnable(false);
                card.setBlackVis(true);
            }
        };

        ///////////////////////////碰扛牌///////////////////////////
        MJCards.prototype.addPengCard = function (pos, type, value, out_pos, OpType, specialCard) {
            var zhongfabaiType = 1;
            var isZhongFaBaiGang = false;
            if ((OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG1) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG1 ||
                (OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG2) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG2 ||
                (OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG3) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG3 ||
                (OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG4) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG4) {
                if ((OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG1) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG1) {
                    zhongfabaiType = 1;
                    isZhongFaBaiGang = true;
                }
                if ((OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG2) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG2) {
                    zhongfabaiType = 2;
                    isZhongFaBaiGang = true;
                }
                if ((OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG3) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG3) {
                    zhongfabaiType = 3;
                    isZhongFaBaiGang = true;
                }
                if ((OpType & ActionType_QiChun.TYPE_ZHONGFABAI_GANG4) == ActionType_QiChun.TYPE_ZHONGFABAI_GANG4) {
                    zhongfabaiType = 4;
                    isZhongFaBaiGang = true;
                }
            }


            if (isZhongFaBaiGang) {
                type = PengType.Chi_M;
                out_pos = -1;
                value = 0x42;
            }

            for (var i = 0; i < zhongfabaiType; i++) {
                if (specialCard) {
                    this.removeHandCard(pos, value, 1);
                    this.removeHandCard(pos, value + 1, 1);
                    this.removeHandCard(pos, value - 1, 1);
                }
                if (type != PengType.Chi_L && type != PengType.Chi_M && type != PengType.Chi_R) {
                    for (var i = 0; i < this.peng_card[pos].length; i++) {
                        var p = this.peng_card[pos][i];
                        //杠的类型 不能重复添加 有特殊要求需覆盖此函数
                        if (p.pengType == type && p.getValue() == value && value != 0) {
                            return false;
                        }
                    }
                }
                var card = new MJPeng();
                this.peng_card[pos].push(card);
                card.setValue(type, pos, value, this.peng_card[pos].length - 1, out_pos, false, OpType, specialCard);
                card.setPosition(MJConfig.getPengKangCardPos(pos, this.peng_card[pos].length - 1));
                card.setScale(MJConfig.getScale_new(pos, CardType.Card_Peng, this.peng_card[pos].length - 1, false));
                this.addChild(card, MJConfig.getPengKangCardOrder(pos, this.peng_card[pos].length - 1));
            }
            return true;
        };

        MJCards.prototype.setSomeCardClickState = function () {

            if (MJModel.mj_lockcard_type == 1) {
                var listen_cards = [];
                for (var i = 0; i < MJModel.listen_info.length; i++) {
