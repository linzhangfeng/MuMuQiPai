    handler_server_scene_info_uc: function (huaibei_gameScene, canDelay) {
        var ackGameFree = huaibei_gameScene.GameSence;
        var sceneStatus = ackGameFree.SceneStatus;
        this._super(ackGameFree, canDelay);
        this._setGameType();
        if (huaibei_gameScene.ispiaoing == 1 ) {
            MJModel.banker_seatid = ackGameFree.BankerChairid;
            MJModel.roomState = 1;
            this.resetGame(false);
            this.room_Info.setVisible(true);
            JsUtils.postNotifi("notify_game_check_end");//为了刷新局数 或者  开始倒计时
            for (var i = 0; i < huaibei_gameScene.userpiao.length; i++) {
                var pos = MJModel.getPosBySeatid(i);
                var piaonum = huaibei_gameScene.userpiao[i];
                if (piaonum == -1) {
                    if(MJModel.isMyPlayer(i)){
                        this._showPiaoLayer();
                    }
                }else{
                    var player = this.players[pos];
                    player.setPiaoNum(piaonum);
                }
            }
            for (var i = 0; i < huaibei_gameScene.userhuo.length; i++) {
                var pos = MJModel.getPosBySeatid(i);
                var zuonum = huaibei_gameScene.userhuo[i];
                if (zuonum == -1) {
                    if(MJModel.isMyPlayer(i)){
                        this._showZuoLayer();
                    }
                }else{
                    var player = this.players[pos];
                    player.setZuoNum(zuonum);
                }
            }
            for(var k=0; k<4; k++){
                var pm = MJModel.players[k];
                if (pm.uid != -1) {
                    var pos = MJModel.getPosBySeatid(pm.seatid);
                    var mp = this.players[pos];
                    mp.setReadyFlag(false);
                }
            }
        }else{
            for (var i = 0; i < huaibei_gameScene.userpiao.length; i++) {
                var pos = MJModel.getPosBySeatid(i);
                var piaonum = huaibei_gameScene.userpiao[i];
                var player = this.players[pos];
                player.setPiaoNum(piaonum);
                }
            for (var i = 0; i < huaibei_gameScene.userhuo.length; i++) {
                var pos = MJModel.getPosBySeatid(i);
                var zuonum = huaibei_gameScene.userhuo[i];
                var player = this.players[pos];
                player.setZuoNum(zuonum);
                }
            }
    },
